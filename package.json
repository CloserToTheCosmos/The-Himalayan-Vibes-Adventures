  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "parcel": "^2.0.0-beta.2",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1"
  }
}
